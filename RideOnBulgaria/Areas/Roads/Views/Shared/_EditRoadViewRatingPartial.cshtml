@model EditRoadViewModel

<fieldset class="rating">
    <input type="radio" asp-for="View" id="star5" value="5" /><label asp-for="View" class="full" for="star5" title="Awesome - 5 stars"></label>
    <input type="radio" asp-for="View" id="star4" value="4" /><label asp-for="View" class="full" for="star4" title="Pretty good - 4 stars"></label>
    <input type="radio" asp-for="View" id="star3" value="3" /><label asp-for="View" class="full" for="star3" title="Meh - 3 stars"></label>
    <input type="radio" asp-for="View" id="star2" value="2" /><label asp-for="View" class="full" for="star2" title="Kinda bad - 2 stars"></label>
    <input type="radio" asp-for="View" id="star1" value="1" /><label asp-for="View" class="full" for="star1" title="Sucks big time - 1 star"></label>
</fieldset>